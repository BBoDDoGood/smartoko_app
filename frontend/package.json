{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start --clear",
    "dev:android": "expo start --android --clear",
    "dev:ios": "expo start --ios --clear",
    "dev:web": "expo start --web --clear",
    "staging": "EXPO_PUBLIC_ENV=staging expo start --clear",
    "staging:android": "EXPO_PUBLIC_ENV=staging expo start --android --clear",
    "staging:ios": "EXPO_PUBLIC_ENV=staging expo start --ios --clear",
    "staging:web": "EXPO_PUBLIC_ENV=staging expo start --web --clear",
    "prod": "EXPO_PUBLIC_ENV=production expo start --clear",
    "prod:android": "EXPO_PUBLIC_ENV=production expo start --android --clear",
    "prod:ios": "EXPO_PUBLIC_ENV=production expo start --ios --clear",
    "prod:web": "EXPO_PUBLIC_ENV=production expo start --web --clear",
    "build:dev": "expo build --profile development",
    "build:staging": "expo build --profile staging",
    "build:prod": "expo build --profile production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
